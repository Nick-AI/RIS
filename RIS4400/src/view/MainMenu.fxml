<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="controller.MainMenuController">
   <children>
      <BorderPane prefHeight="302.0" prefWidth="427.0">
         <center>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="ButtonRegistration" mnemonicParsing="false" onAction="#registration" text="Registration" GridPane.rowIndex="1" />
                  <Button fx:id="ButtonImaging" mnemonicParsing="false" onAction="#imaging" text="Imaging" GridPane.rowIndex="2" />
                  <Button fx:id="ButtonVitals" mnemonicParsing="false" onAction="#vitals" text="Vitals" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Button fx:id="ButtonFiles" mnemonicParsing="false" onAction="#files" text="Files and Review" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
